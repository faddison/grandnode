            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor - Derive ProductAttributeValue" enabled="true">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">

for (int i; i &lt; 10; i++)
{
	String attributeName = &quot;ProductAttributeName_&quot;+i;
	String attributeValue = vars.get(attributeName);
	if (attributeValue != null)
	{
		log.info(attributeValue);
		
		String newAttributeValue = attributeValue.replace(&quot;label_&quot;,&quot;&quot;);
		vars.put(attributeName, newAttributeValue);
		
		String attributeId = attributeValue.split(&quot;_&quot;)[3];
		log.info(attributeId);

		String attributeWithoutHex = attributeId.substring(0, attributeId.length()-4);
		String hexString = attributeId.substring(attributeId.length()-4,attributeId.length());
		log.info(hexString);

		int hexValue = Integer.parseInt(hexString,16);
		log.info(hexValue.toString());

		int newHexValue = hexValue+2;
		String newHexString = Integer.toHexString(newHexValue);
		log.info(newHexString);		
				
		String selectedName = &quot;ProductAttributeValue_&quot;+i;

		vars.put(selectedName, attributeWithoutHex+newHexString);

		log.info(&quot;\n&quot;);
	}	
}
</stringProp>
            </BeanShellPostProcessor>
